{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-king"
		},
		"LS_AWS_S3_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'LS_AWS_S3'"
		},
		"LS_StorageAccount_Gen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_StorageAccount_Gen2'"
		},
		"LS_AWS_S3_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIATHJS2RKPDTX4CVH3"
		},
		"LS_StorageAccount_Gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://manoj1king.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-king_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AWS_S3_secretAccessKey": {
						"value": "[parameters('LS_AWS_S3_secretAccessKey')]"
					},
					"LS_StorageAccount_Gen2_accountKey": {
						"value": "[parameters('LS_StorageAccount_Gen2_accountKey')]"
					},
					"LS_AWS_S3_properties_typeProperties_accessKeyId": {
						"value": "[parameters('LS_AWS_S3_properties_typeProperties_accessKeyId')]"
					},
					"LS_StorageAccount_Gen2_properties_typeProperties_url": {
						"value": "[parameters('LS_StorageAccount_Gen2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}